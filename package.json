{
  "name": "ts-common",
  "version": "0.2.0",
  "private": true,
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "build": "pnpm -r --sequential build",
    "test": "pnpm -r test",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.cache/eslint",
    "lint:ci": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "pnpm -r clean",
    "rebuild": "pnpm clean && pnpm build",
    "changeset": "changeset",
    "release": "changeset version && pnpm install && pnpm build && changeset publish"
  },
  "pnpm": {
    "overrides": {
      "@fabianopinto/*": "workspace:*"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4"
  }
}
