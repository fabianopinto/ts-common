{
  "name": "ts-common",
  "version": "0.1.0",
  "private": true,
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "build": "pnpm -r --stream build",
    "test": "pnpm -r test",
    "lint": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.cache/eslint",
    "lint:ci": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "pnpm -r clean",
    "rebuild": "pnpm run clean && pnpm run build",
    "changeset": "changeset",
    "release": "changeset version && pnpm install --no-frozen-lockfile && pnpm install -r --no-frozen-lockfile && pnpm -r build && changeset publish --no-git-tag",
    "pkg:build": "tsup",
    "pkg:dev": "tsup --watch",
    "pkg:clean": "rimraf dist || rm -rf dist",
    "pkg:lint": "eslint src --ext .ts,.tsx --cache --cache-location ../../node_modules/.cache/eslint",
    "pkg:test": "echo 'no tests yet'"
  },
  "pnpm": {
    "overrides": {
      "@fabianopinto/*": "workspace:*"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "tsup": "^8.5.0",
    "typescript": "~5.9.2",
    "vitest": "^1.6.1"
  }
}
