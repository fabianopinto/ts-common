{
  "name": "@fabianopinto/utils",
  "version": "0.1.0",
  "private": false,
  "license": "ISC",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@fabianopinto/errors": "workspace:*",
    "@fabianopinto/logger": "workspace:*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist || rm -rf dist",
    "lint": "echo 'no lint configured'",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
