{
  "name": "@fabianopinto/logger",
  "version": "0.1.0",
  "private": false,
  "license": "ISC",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist || rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepack": "pnpm run clean && pnpm run build"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabianopinto/ts-common.git",
    "directory": "packages/logger"
  },
  "bugs": {
    "url": "https://github.com/fabianopinto/ts-common/issues"
  },
  "homepage": "https://github.com/fabianopinto/ts-common#readme",
  "dependencies": {
    "@fabianopinto/errors": "workspace:*"
  },
  "peerDependencies": {
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1"
  },
  "peerDependenciesMeta": {
    "pino-pretty": {
      "optional": true
    }
  },
  "devDependencies": {
    "pino": "^9.9.5",
    "pino-pretty": "^13.1.1"
  }
