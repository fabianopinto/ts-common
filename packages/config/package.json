{
  "name": "@fabianopinto/config",
  "version": "0.2.1",
  "private": false,
  "license": "ISC",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist || rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabianopinto/ts-common.git",
    "directory": "packages/config"
  },
  "bugs": {
    "url": "https://github.com/fabianopinto/ts-common/issues"
  },
  "homepage": "https://github.com/fabianopinto/ts-common#readme",
  "dependencies": {
    "@fabianopinto/errors": "workspace:*",
    "@fabianopinto/logger": "workspace:*",
    "@fabianopinto/utils": "workspace:*"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.887.0",
    "@aws-sdk/client-ssm": "^3.887.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-s3": {
      "optional": true
    },
    "@aws-sdk/client-ssm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.887.0",
    "@aws-sdk/client-ssm": "^3.887.0"
  }
}
